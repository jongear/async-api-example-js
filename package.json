{
  "name": "async-api-example",
  "version": "1.0.0",
  "description": "A starter async Node application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jongear/async-api-example-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jongear/async-api-example-js/issues"
  },
  "homepage": "https://github.com/jongear/async-api-example-js#readme",
  "scripts": {
    "lint": "eslint .",
    "lint:watch": "watch 'npm run lint'",
    "start": "node src/server",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js src/server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "report-coverage": "npm run test:coverage && cat ./coverage/lcov.info | codecov"
  },
  "jest": {
    "testURL": "http://localhost",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**src/config/*.js",
      "!**src/config/*/*.js",
      "!**src/docs/*.js",
      "!**src/models/*.js"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.3.0",
    "swagger-ui-express": "^4.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "codecov": "^3.6.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.6.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
